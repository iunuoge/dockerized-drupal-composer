{
    "name": "squareweave/drupal-composer",
    "description": "Project template for Drupal 8 projects with composer and Docker",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "composer/installers": "^1.0.20"
    },
    "replace": {
        "drupal/core": "8.*",
        "drupal/drupal": "8.*",
        "drupal/action": "^8.1.1",
        "drupal/aggregator": "^8.1.1",
        "drupal/automated_cron": "^8.1.1",
        "drupal/bartik": "^8.1.1",
        "drupal/ban": "^8.1.1",
        "drupal/basic_auth": "^8.1.1",
        "drupal/big_pipe": "^8.1.1",
        "drupal/block": "^8.1.1",
        "drupal/block_content": "^8.1.1",
        "drupal/book": "^8.1.1",
        "drupal/breakpoint": "^8.1.1",
        "drupal/ckeditor": "^8.1.1",
        "drupal/classy": "^8.1.1",
        "drupal/color": "^8.1.1",
        "drupal/comment": "^8.1.1",
        "drupal/config": "^8.1.1",
        "drupal/config_translation": "^8.1.1",
        "drupal/contact": "^8.1.1",
        "drupal/content_translation": "^8.1.1",
        "drupal/contextual": "^8.1.1",
        "drupal/core-annotation": "^8.1.1",
        "drupal/core-bridge": "^8.1.1",
        "drupal/core-datetime": "^8.1.1",
        "drupal/core-diff": "^8.1.1",
        "drupal/core-discovery": "^8.1.1",
        "drupal/core-event-dispatcher": "^8.1.1",
        "drupal/core-file-cache": "^8.1.1",
        "drupal/core-gettext": "^8.1.1",
        "drupal/core-graph": "^8.1.1",
        "drupal/core-php-storage": "^8.1.1",
        "drupal/core-plugin": "^8.1.1",
        "drupal/core-proxy-builder": "^8.1.1",
        "drupal/core-serialization": "^8.1.1",
        "drupal/core-transliteration": "^8.1.1",
        "drupal/core-utility": "^8.1.1",
        "drupal/core-uuid": "^8.1.1",
        "drupal/datetime": "^8.1.1",
        "drupal/dblog": "^8.1.1",
        "drupal/dynamic_page_cache": "^8.1.1",
        "drupal/editor": "^8.1.1",
        "drupal/entity_reference": "^8.1.1",
        "drupal/field": "^8.1.1",
        "drupal/field_ui": "^8.1.1",
        "drupal/file": "^8.1.1",
        "drupal/filter": "^8.1.1",
        "drupal/forum": "^8.1.1",
        "drupal/hal": "^8.1.1",
        "drupal/help": "^8.1.1",
        "drupal/history": "^8.1.1",
        "drupal/image": "^8.1.1",
        "drupal/inline_form_errors": "^8.1.1",
        "drupal/language": "^8.1.1",
        "drupal/link": "^8.1.1",
        "drupal/locale": "^8.1.1",
        "drupal/minimal": "^8.1.1",
        "drupal/menu_link_content": "^8.1.1",
        "drupal/menu_ui": "^8.1.1",
        "drupal/migrate": "^8.1.1",
        "drupal/migrate_drupal": "^8.1.1",
        "drupal/migrate_drupal_ui": "^8.1.1",
        "drupal/node": "^8.1.1",
        "drupal/options": "^8.1.1",
        "drupal/page_cache": "^8.1.1",
        "drupal/path": "^8.1.1",
        "drupal/quickedit": "^8.1.1",
        "drupal/rdf": "^8.1.1",
        "drupal/responsive_image": "^8.1.1",
        "drupal/rest": "^8.1.1",
        "drupal/search": "^8.1.1",
        "drupal/serialization": "^8.1.1",
        "drupal/seven": "^8.1.1",
        "drupal/shortcut": "^8.1.1",
        "drupal/simpletest": "^8.1.1",
        "drupal/standard": "^8.1.1",
        "drupal/stark": "^8.1.1",
        "drupal/statistics": "^8.1.1",
        "drupal/syslog": "^8.1.1",
        "drupal/system": "^8.1.1",
        "drupal/taxonomy": "^8.1.1",
        "drupal/telephone": "^8.1.1",
        "drupal/text": "^8.1.1",
        "drupal/toolbar": "^8.1.1",
        "drupal/tour": "^8.1.1",
        "drupal/tracker": "^8.1.1",
        "drupal/update": "^8.1.1",
        "drupal/user": "^8.1.1",
        "drupal/views": "^8.1.1",
        "drupal/views_ui": "^8.1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "/var/www/html/modules/contrib/{$name}": ["type:drupal-module"],
            "/var/www/html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "/var/www/html/themes/contrib/{$name}": ["type:drupal-theme"]
        }
    }
}